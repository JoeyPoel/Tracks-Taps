{
  "expo": {
    "name": "Tracks&Taps",
    "slug": "tracks-and-taps",
    "version": "2.2.2",
    "orientation": "portrait",
    "icon": "./assets/images/App Icon Coloured.png",
    "scheme": "tracks-taps",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "web": {
      "bundler": "metro",
      "output": "server"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.joeypoel.trackstaps",
      "buildNumber": "6",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Tracks Taps uses your location to show your position on the map and verify location-based challenges.",
        "NSPhotoLibraryUsageDescription": "Tracks Taps needs access to your photo library to select images for tours and stops.",
        "NSCameraUsageDescription": "Tracks Taps needs access to your camera to take photos for challenges and tour images.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/App Icon Coloured.png"
      },
      "package": "com.joeypoel.trackstaps",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Splash Screen Coloured.png",
          "imageWidth": 1024,
          "imageHeight": 1024,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.200375813162-21etd79sc2q5276t3aer5c5f7kfg1aqb"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "033de2f9-fb8a-489a-8360-f4fb5f48540e"
      }
    },
    "owner": "tracks-and-taps"
  }
}