{
  "expo": {
    "name": "Tracks&Taps",
    "slug": "tracks-and-taps",
    "version": "1.6.0",
    "orientation": "portrait",
    "icon": "./assets/images/App Icon Coloured.png",
    "scheme": "tracks-taps",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.joeypoel.trackstaps",
      "buildNumber": "4",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Tracks Taps uses your location to show your position on the map and verify location-based challenges.",
        "NSPhotoLibraryUsageDescription": "Tracks Taps needs access to your photo library to select images for tours and stops.",
        "NSCameraUsageDescription": "Tracks Taps needs access to your camera to take photos for challenges and tour images.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "foregroundImage": "./assets/images/App Icon Coloured.png"
      },
      "package": "com.joeypoel.trackstaps",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "output": "server",
      "favicon": "./assets/images/App Icon Coloured.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Splash Screen Coloured.png",
          "imageWidth": 1024,
          "imageHeight": 1024,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-font",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.135339006032-7pf4smvcht4pj54tu2s03mn0b887jpru"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8ff732e5-d191-421d-a9f3-c170186ec961"
      }
    },
    "owner": "joey_poel"
  }
}